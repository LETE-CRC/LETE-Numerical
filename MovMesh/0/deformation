FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      deformation;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Out
    {
        type            fixedValue;
        value           uniform (1 1 1);
    }
    frontAndBack
    {
        type            empty;
    }
    topAndBottom
    {
        type            empty;
    }
    left
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


